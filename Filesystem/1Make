CXX=g++
BOOSTPATH=/home/kshi/Boost/boost_1_65_0
LIBS= -I /home/kshi/Boost/boost_1_65_0
LIBS+= -L /home/kshi/Boost/boost_1_65_0/stage/lib/ -lboost_system -lboost_filesystem

binPath: Path.cpp
	$(CXX) -o binPath Path.cpp $(LIBS) 
clean:
	rm binPath

